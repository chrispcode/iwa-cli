{
  "name": "iwa-cli",
  "description": "An ImmutableWebApp CLI using oclif and cosmiconfig",
  "version": "1.0.0",
  "author": "Kristiyan Serafimov (chrispcode)",
  "repository": "chrispcode/iwa-cli",
  "files": [
    "/bin",
    "/lib"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "homepage": "https://github.com/chrispcode/iwa-cli",
  "keywords": [
    "oclif",
    "iwa",
    "ImmutableWebApps",
    "cli"
  ],
  "license": "MIT",
  "bin": {
    "iwa": "./bin/run"
  },
  "bugs": "https://github.com/chrispcode/iwa-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "chalk": "^4.1.0",
    "cosmiconfig": "^7.0.0",
    "lodash.pick": "^4.4.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.14.0",
    "@oclif/test": "^1.2.8",
    "@types/common-tags": "^1.8.0",
    "@types/jest": "^27.0.1",
    "@types/lodash.pick": "^4.4.6",
    "@types/node": "^14.14.25",
    "@typescript-eslint/parser": "^4.14.2",
    "common-tags": "^1.8.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "oclif": {
    "bin": "iwa",
    "commands": "./lib/commands"
  },
  "scripts": {
    "prepack": "rm -rf lib && tsc -b && oclif-dev readme",
    "test": "jest",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "lint:fix": "eslint . --ext .ts --config .eslintrc --fix",
    "version": "oclif-dev readme && git add README.md",
    "test:cli:gen": "node ./bin/run gen example/index.html",
    "test:cli:rm": "node ./bin/run rm example/index.html",
    "test:cli:help": "node ./bin/run --help"
  }
}
